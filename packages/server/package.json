{
  "name": "@sodazone/ocelloids-service-node",
  "version": "3.0.1-dev.0",
  "type": "module",
  "description": "Ocelloids Service Node",
  "author": "SO/DA <projects@soda.zone>",
  "contributors": [
    "Marc Forn√≥s <marc@soda.zone>",
    "Xueying Wang <xueying@soda.zone>"
  ],
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "bin": {
    "oc-ingress": "./dist/services/ingress/server/main.js",
    "oc-node": "./dist/main.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "prod:build": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "start": "node ./dist/main.js",
    "dev": "nodemon --exec tsx ./src/main.ts",
    "dev:inspect": "nodemon --exec tsx --inspect ./src/main.ts",
    "dev:ingress": "nodemon --exec tsx ./src/services/ingress/server/main.ts",
    "dev:exec": "nodemon --exec tsx ./src/main.ts --distributed",
    "lint": "biome check --write src/**/*.ts",
    "lint:fix": "biome check --fix --unsafe src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/node": "^24.7.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.4.1-r.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/websocket": "^11.2.0",
    "@polkadot-api/legacy-provider": "0.3.3",
    "@polkadot-api/tx-utils": "0.1.7",
    "@scalar/fastify-api-reference": "^1.38.0",
    "@types/better-sqlite3": "^7.6.13",
    "@wormhole-foundation/sdk-definitions": "^3.8.5",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^12.4.1",
    "bs58": "^6.0.0",
    "cbor-x": "^1.6.0",
    "close-with-grace": "^2.3.0",
    "commander": "^14.0.1",
    "destr": "^2.0.5",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "fastify-healthcheck": "^5.1.0",
    "fastify-plugin": "^5.1.0",
    "got": "^14.4.9",
    "handlebars": "^4.7.8",
    "kysely": "^0.28.7",
    "level": "^10.0.0",
    "lru-cache": "^11.2.2",
    "memory-level": "^3.1.0",
    "mingo": "6.7.0",
    "p-limit": "^7.1.1",
    "polkadot-api": "1.20.0",
    "prom-client": "^15.1.3",
    "redis": "^5.8.3",
    "rfc6902": "^5.1.2",
    "rxjs": "^7.8.2",
    "toml": "^3.0.0",
    "ulidx": "^2.4.1",
    "viem": "^2.38.0",
    "ws": "^8.18.3",
    "zod": "^3.24.6",
    "zod-to-json-schema": "^3.24.6"
  },
  "packageManager": "yarn@4.1.0",
  "stableVersion": "3.0.0"
}
