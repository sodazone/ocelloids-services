{
  "name": "@sodazone/ocelloids-service-node",
  "version": "2.4.1-dev.0",
  "type": "module",
  "description": "Ocelloids Service Node",
  "author": "SO/DA <projects@soda.zone>",
  "contributors": [
    "Marc Forn√≥s <marc@soda.zone>",
    "Xueying Wang <xueying@soda.zone>"
  ],
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "bin": {
    "oc-ingress": "./dist/services/ingress/server/main.js",
    "oc-node": "./dist/main.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "prod:build": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "start": "node ./dist/main.js",
    "dev": "nodemon --exec tsx ./src/main.ts",
    "dev:inspect": "nodemon --exec tsx --inspect ./src/main.ts",
    "dev:ingress": "nodemon --exec tsx ./src/services/ingress/server/main.ts",
    "dev:exec": "nodemon --exec tsx ./src/main.ts --distributed",
    "lint": "biome check --write src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.10.7",
    "@types/ws": "^8.5.13",
    "@vitest/coverage-v8": "3.0.2",
    "@vitest/ui": "3.0.2",
    "nock": "^13.5.6",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/websocket": "^11.0.2",
    "@polkadot-api/tx-utils": "^0.0.8",
    "@scalar/fastify-api-reference": "^1.25.105",
    "@types/better-sqlite3": "^7.6.12",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^11.8.1",
    "cbor-x": "^1.6.0",
    "close-with-grace": "^2.2.0",
    "commander": "^13.0.0",
    "destr": "^2.0.3",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-healthcheck": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "got": "^14.4.5",
    "handlebars": "^4.7.8",
    "kysely": "^0.27.5",
    "level": "^9.0.0",
    "lru-cache": "^11.0.2",
    "memory-level": "^2.0.0",
    "mingo": "^6.5.2",
    "polkadot-api": "^1.8.2",
    "prom-client": "^15.1.3",
    "rave-level": "^1.0.0",
    "redis": "^4.7.0",
    "rfc6902": "^5.1.2",
    "rxjs": "^7.8.1",
    "toml": "^3.0.0",
    "ulidx": "^2.4.1",
    "viem": "^2.22.10",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "packageManager": "yarn@4.1.0",
  "stableVersion": "2.4.0"
}
